{
  "presets": [
    ["@stellar-apps/react", {"removePropTypes": false}]
  ],
  "env": {
    "cjs": {
      "presets": [
        [
          "@stellar-apps/es",
          {
            "env": {
              "targets": {"chrome": "41", "ie": "11"}
            },
            "runtime": {"corejs": 2}
          }
        ]
      ]
    },
    "es": {
      "presets": [
        [
          "@stellar-apps/es",
          {
            "env": {
              "useBuiltIns": false,
              "modules": false,
              "targets": {"browsers": ">5% in US"}
            },
            "runtime": {"useESModules": true}
          }
        ]
      ]
    },
    "node-es": {
      "presets": [
        [
          "@stellar-apps/es",
          {
            "env": {
              "useBuiltIns": false,
              "modules": false,
              "targets": {"node": "8"}
            },
            "runtime": {"useESModules": true}
          }
        ]
      ]
    }
  }
}
